            getCheckboxCollection('input[id^=mbb-offeringID-]').uncheck();
          
        
        getCheckboxCollection('input[id^=mbb-offeringID-1]').check();
        logRefTag('psd_bb_y_B00UZKG8QU_B011WVZKKA');
        hidePopover("1");
      });
      $(document).undelegate('#mbbNoCoverage1', 'click');
      $(document).delegate('#mbbNoCoverage1', 'click', function() {
        getCheckboxCollection('input[id^=mbb-offeringID-1]').uncheck();
